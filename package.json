{
  "name": "bitbucket-to-sheets",
  "version": "1.0.0",
  "description": "Sync Bitbucket commits to Google Sheets",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:server": "node dist/server.js",
    "dev": "ts-node src/index.ts",
    "dev:server": "ts-node src/server.ts",
    "server": "ts-node src/server.ts",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^5.2.1",
    "google-auth-library": "^9.4.0",
    "googleapis": "^129.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  }
}